        Get
            Return _SimVars
        End Get
    End Property

    Public ReadOnly Property SimVarsName As Dictionary(Of String, SimVar)
        Get
            Return _SimVarsName
        End Get
    End Property

    Public ReadOnly Property SimVarNames As String()
        Get
            Return _SimVarsName.Keys.ToArray
        End Get
    End Property

    Public ReadOnly Property BaseSimVars As List(Of SimVarEnum)
        Get
